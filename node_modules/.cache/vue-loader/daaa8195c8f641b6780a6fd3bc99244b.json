\n</template>\r\n\r\n<script>\r\nvar speech = new SpeechSynthesisUtterance();\r\nspeech.rate = 1;\r\nspeech.pitch = 1;\r\nspeech.volume = 1;\r\nspeech.voice = speechSynthesis.getVoices()[0];\r\n\r\nexport default {\r\n  name: \"TextVoiceApp\",\r\n\r\n  methods: {\r\n    speakInputText() {\r\n      // Esta funcion convierte en audio el texto escrito.\r\n      let text = document.getElementById(\"textArea\").value;\r\n      if (text != \"\") {\r\n        //Este condicional determina, si no hay nada escrito te pide ingresar algo, en el caso contrario se reproduce lo escrito.\r\n        speech.text = document.getElementById(\"textArea\").value;\r\n        speechSynthesis.speak(speech);\r\n      } else {\r\n        text = \"No has ingresado nada, por favor escribe algo\";\r\n        speech.text = text;\r\n        speechSynthesis.speak(speech);\r\n      }\r\n    },\r\n    stopText() {\r\n      speechSynthesis.cancel();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container-all {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 750px;\r\n  background-image: url(\"./assets/back3.jpg\");\r\n}\r\n\r\n.container-logo {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n  margin-bottom: 50px;\r\n}\r\n\r\nimg {\r\n  width: 300px;\r\n}\r\n\r\n.btn {\r\n  width: 120px;\r\n  height: 35px;\r\n  font-size: 16px;\r\n  border-radius: 20px;\r\n  border-color: rgba(255, 255, 255, 0.1);\r\n  outline-color: rgba(255, 255, 255, 0.1);\r\n  margin-top: 40px;\r\n  font-family: monospace;\r\n}\r\n\r\n.container-all textarea {\r\n  width: 42%;\r\n  height: 50%;\r\n  min-width: 42%;\r\n  max-width: 42%;\r\n  min-height: 50%;\r\n  max-height: 50%;\r\n  padding-left: 12px;\r\n  padding-top: 12px;\r\n  margin-top: 12px;\r\n  border-radius: 20px;\r\n  font-size: 22px;\r\n  background: rgba(255, 255, 255, 0.2);\r\n  outline-color: rgba(255, 255, 255, 0.1);\r\